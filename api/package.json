{
  "name": "api",
  "version": "0.0.1",
  "description": "BridgeTower api service by NestJS",
  "author": "IdeaSoft",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": " nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main.js",
    "start:docker": "yarn db:migrate && yarn db:seed && yarn start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint --quiet --fix \"src/**/*.{ts,json}\"",
    "lint:staged": "lint-staged",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "doc": "compodoc -p tsconfig.json -s -d docs",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all",
    "predev": "dotenv-checker --schema .env.schema --env .env"
  },
  "dependencies": {
    "@golevelup/nestjs-rabbitmq": "^3.2.0",
    "@liaoliaots/nestjs-redis": "^9.0.1",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/sequelize": "^9.0.0",
    "@nestjs/swagger": "^6.1.0",
    "@nestjs/terminus": "^9.1.1",
    "@truffle/hdwallet-provider": "^2.0.14",
    "@types/axios": "^0.14.0",
    "@types/morgan": "^1.9.3",
    "@types/streamifier": "^0.1.0",
    "amqplib": "^0.10.3",
    "axios": "^0.27.2",
    "bignumber.js": "^9.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cloudinary": "^1.31.0",
    "compression": "^1.7.4",
    "config": "^3.3.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "ethereum-protocol": "^1.0.1",
    "express": "^4.18.1",
    "express-basic-auth": "^1.2.1",
    "helmet": "^6.0.0",
    "ioredis": "^5.2.3",
    "mariadb": "^3.0.1",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sequelize": "^6.21.4",
    "sequelize-cli": "^6.4.1",
    "sequelize-typescript": "^2.1.3",
    "shorthash2": "^1.0.3",
    "streamifier": "^0.1.1",
    "uuid": "^9.0.0",
    "web3": "^1.7.5",
    "web3-eth-contract": "^1.7.5",
    "web3-utils": "^1.7.5"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.19",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/amqplib": "^0.8.2",
    "@types/compression": "^1.7.2",
    "@types/config": "^3.3.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.4",
    "@types/knex": "^0.16.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.6.4",
    "@types/passport-local": "^1.0.34",
    "@types/sequelize": "^4.28.14",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "dotenv-checker": "^1.1.5",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-airbnb-base": "^0.0.1-security",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-mocha": "^10.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^8.0.1",
    "jest": "28.1.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.20",
    "standard-version": "^9.5.0",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "~4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "main": "dist/app",
    "engines": {
      "node": ">=16"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "src/**/*.{ts}": [
      "npm run format",
      "npm run lint"
    ]
  }
}
