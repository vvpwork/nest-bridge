    "cloudinary": "^1.32.0",
    "config": "^3.3.7",
    "dotenv": "^16.0.2",
    "dotenv-checker": "^1.1.5",
    "mariadb": "^3.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sequelize": "^6.21.4",
