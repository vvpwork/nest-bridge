    "test:e2e": "jest --config ./test/jest-e2e.json",
    "predev": "dotenv-checker --schema .env.schema --env .env"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
